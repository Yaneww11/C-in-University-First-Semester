#include <iostream>
#include<algorithm>
#include<math.h>

using namespace std;

int main()
{
  int n;


  cout << "Enter a number: ";
  cin >> n;

  if(!cin || n <=0){
    cout << "Wrong output";
    return 1;
  }

  int arr[n];

  for(int i = 0; i < n; i++){
   arr[i] =pow(2,i);


}
for (int i =0; i<n; i++){
    cout << "arr[" << i << "] = " << arr[i] << endl;
}









    return 0;
}
